#ifndef ENU_FRAME_H_
#define ENU_FRAME_H_

#include "WGS84_Frame.h"

class CENU_Frame : public CWGS84_Frame
{
public:
	CENU_Frame();
	CENU_Frame(const CENU_Frame &enu_f);
	CENU_Frame(long double x_enu, long double y_enu, long double z_enu, CWGS84_Frame wgs84_origin);
	CENU_Frame(CWGS84_Frame wgs84_f, CWGS84_Frame wgs84_origin);
	virtual ~CENU_Frame();

protected:
	CWGS84_Frame wgs84_origin;
};

#endif /* ENU_FRAME_H_ */
