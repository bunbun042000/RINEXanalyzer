#ifndef ECEF_FRAME_H_
#define ECEF_FRAME_H_

class CECEF_Frame
{
friend CECEF_Frame operator-(CECEF_Frame &f1, CECEF_Frame &f2);

public:
	CECEF_Frame();
	CECEF_Frame(long double x, long double y, long double z);
	~CECEF_Frame();

private:
	long double x;
	long double y;
	long double z;

};


#endif /* ECEF_FRAME_H_ */
