#include "GPS_Time_Week.h"
#include <time.h>

GPS_Time_Week::GPS_Time_Week()
{
  week = 0;
  second = 0.0;
}

GPS_Time_Week::GPS_Time_Week(int w, double sec)
{
  week = w;
  second = sec;
}

GPS_Time_Week::~GPS_Time_Week()
{
  // Do nothing
}

tm GPS_Time_Week::ToDate()
{
  time_t t;

  t = (long) week * Seconds_in_Week + GPS_Week_Origin
     +(long)((second>0.0)?second+0.5:second-0.5);
  return *gmtime(&t);
}



