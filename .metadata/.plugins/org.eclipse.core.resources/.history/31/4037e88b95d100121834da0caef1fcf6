#include <math.h>
#include "ECEF_Frame.h"

CECEF_Frame::CECEF_Frame()
{
	x = 0.0;
	y = 0.0;
	z = 0.0;
}
CECEF_Frame::CECEF_Frame(long double x0, long double y0, long double z0)
{
	x = x0;
	y = y0;
	z = z0;
}

CECEF_Frame::~CECEF_Frame()
{
	// Do nothing
}

long double CECEF_Frame operator-(CECEF_Frame &f1, CECEF_Frame &f2)
{
	return sqrtl(powl((f2.x - f1.x), 2) + powl((f2.y - f1.y), 2) + powl((f2.z - f1.z),2));
}



